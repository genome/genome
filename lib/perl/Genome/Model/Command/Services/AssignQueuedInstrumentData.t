sub _add_instrument_data_to_projects {
    my $instrument_data = shift;
    for my $project ( @projects ) { 
        $project->add_part(
            entity_id => $instrument_data->id, 
            entity_class_name => 'Genome::InstrumentData',
            label => 'instrument_data',
